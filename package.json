{
  "name": "NativeForms",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios-open": "open ios/*.xcodeproj",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "link": "node node_modules/react-native/local-cli/cli.js link",
    "lint": "eslint src test *.js",
    "test": "jest",
    "remotedev": "remotedev-debugger --injectserver",
    "postinstall": "npm run postinstall-js && npm run link && npm run remotedev && rndebugger-open",
    "postinstall-js": "node scripts/postinstall.js",
    "esdoc": "./node_modules/.bin/esdoc"
  },
  "remotedev": {
    "hostname": "localhost",
    "port": 5678
  },
  "jest": {
    "preset": "react-native",
    "snapshotSerializers": [
      "./node_modules/jest-serializer-enzyme"
    ],
    "globals": {
      "__DEV__": true
    }
  },
  "dependencies": {
    "babel-plugin-inline-import": "^2.0.4",
    "color": "^1.0.3",
    "immutable": "^3.7.6",
    "intl": "^1.2.5",
    "lodash": "^4.3.0",
    "native-base": "2.1.0",
    "react": "~15.4.1",
    "react-intl": "^2.3.0",
    "react-native": "^0.42.0",
    "react-native-syntax-highlighter": "^1.2.1",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "^5.0.4",
    "react-syntax-highlighter": "^5.6.2",
    "redux": "^3.3.1",
    "redux-immutable": "^4.0.0",
    "redux-observable": "^0.14.1",
    "render-if": "^0.1.1",
    "rxjs": "^5.3.1",
    "text": "github:requirejs/text"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-remove-console": "^6.8.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "chai": "^4.0.2",
    "enzyme": "^2.0.0",
    "esdoc": "^0.5.2",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.0.0",
    "immutable-devtools": "0.0.7",
    "jest": "^20.0.0",
    "jest-cli": "^20.0.0",
    "jest-enzyme": "^3.1.0",
    "jest-serializer-enzyme": "^1.0.0",
    "node-fetch": "^1.3.3",
    "proxyquire": "^1.7.4",
    "react-addons-test-utils": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-native-debugger-open": "^0.3.0",
    "react-native-mock": "^0.3.1",
    "redux-mock-store": "^1.2.3",
    "remote-redux-devtools": "0.5.0",
    "remote-redux-devtools-on-debugger": "^0.7.0",
    "sinon": "^2.0.0"
  }
